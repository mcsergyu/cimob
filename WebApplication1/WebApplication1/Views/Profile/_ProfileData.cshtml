@using Cimob.Models.ProfileViewModels
@model ProfileViewModel

<div class="box box-primary">
    <div class="box-body">
        <form enctype="multipart/form-data" asp-action="Index" method="get">
            <div class="form-group">
                <img class="img-responsive img-circle" src="~/images/avatar.png" alt="User profile picture" />
            </div>
            <div class="form-group">
                <h3>@Model.Name</h3>
            </div>
            <div class="form-group">
                <h4></h4>
            </div>
        </form>


        @*<p class="text-muted text-center">@Model.</p>*@
    </div>
</div>
